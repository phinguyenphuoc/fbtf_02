<div class="col-lg-12 grid-margin stretch-card">
  <div class="card">
    <div class="card-body">
      <div>
         <%= link_to t(".link_export"), admin_export_users_path(format: :csv), class: "btn btn-primary btn-lg" %>
      </div>
      <br>
      <div class="table-responsive">
        <table class="table table-hover">
          <thead>
            <tr>
              <th><%= t ".id" %></th>
              <th><%= t ".email" %></th>
              <th><%= t ".phonenumber" %></th>
              <th></th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <%= render @users %>
          </tbody>
        </table>
        <%= paginate @users %>
      </div>
    </div>
  </div>
</div>
